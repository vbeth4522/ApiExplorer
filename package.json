{
  "name": "ApiExplorer",
  "version": "1.0.0",
  "description": "UI for CAPI.",
  "main": "main.js",
  "scripts": {
    "watchtest": "karma start --browsers PhantomJS",
    "test": "karma start --browsers PhantomJS --singleRun true",
    "buildjs": "browserify -t [ browserify-ngannotate --add ] src/main.js | sed \"s/[\\\"']ngInject[\\\"'];*//g\" > bundle.js",
    "compilejs": "java -jar ./node_modules/google-closure-compiler/compiler.jar --js bundle.js --js_output_file bundle-min.js --jscomp_off=misplacedTypeAnnotation",
    "buildcss": "node-sass --recursive --output styles --output-style compressed styles/*.sass",
    "build": "npm run buildjs && npm run compilejs && npm run buildcss",
    "start": "electron main.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.4.8",
    "angular-breadcrumb": "^0.4.1",
    "angular-drag-and-drop-lists": "^1.3.2",
    "angular-loading-bar": "^0.8.0",
    "angular-resource": "^1.4.8",
    "angular-sanitize": "^1.4.8",
    "angular-ui-bootstrap": "^0.14.3",
    "angular-ui-router": "^0.2.15",
    "bootstrap": "^3.3.6",
    "browserify": "^12.0.1",
    "browserify-ngannotate": "^2.0.0",
    "electron": "^0.4.1",
    "electron-builder": "^5.0.2",
    "google-closure-compiler": "^20151216.1.0",
    "http-server": "^0.9.0",
    "jquery": "^2.2.4",
    "less": "^2.5.3",
    "lodash": "^3.10.1",
    "menu": "^0.2.5",
    "node-sass": "^3.7.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.4.1",
    "coveralls": "^2.11.9",
    "electron-prebuilt": "^1.2.1",
    "karma": "^0.13.16",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.4",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-sinon-chai": "^1.1.0",
    "lolex": "^1.5.0",
    "mocha": "^2.3.4",
    "phantomjs": "^2.1.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "build": {
    "productName": "ApiExplorer",
    "app-bundle-id": "com.janrain.ApiExplorer",
    "app-category-type": "public.app-category.productivity",
    "osx": {
      "title": "ApiExplorer",
      "identity": "Janrain",
      "entitlements": "build/entitlements.osx.plist",
      "entitlementsInherit": "build/entitlements.osx.inherit.plist"
    }
  }
}
